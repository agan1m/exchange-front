{
  "name": "react-start",
  "version": "0.0.2",
  "description": "react-start react js application",
  "repository": {
    "type": "subversion"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Vitaliy",
  "scripts": {
    "dev": "cross-env NODE_ENV=production TARGET_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build": "cross-env NODE_ENV=production TARGET_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:prestage": "cross-env NODE_ENV=production TARGET_ENV=prestage webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:stage": "cross-env NODE_ENV=production TARGET_ENV=stage webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development TARGET_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development TARGET_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production TARGET_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint ./app/**/*.js",
    "lint:fix": "eslint ./app/**/*.js --fix",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
